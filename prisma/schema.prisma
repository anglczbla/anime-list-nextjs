generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "mysql"

}

model Collection{
  id                  Int @id @default(autoincrement())
  anime_mal_id        String
  user_email          String

  @@unique([user_email, anime_mal_id])
}

